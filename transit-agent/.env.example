ANTHROPIC_API_KEY=
ELEVENLABS_API_KEY=
ELEVENLABS_VOICE_ID=
WHISPER_MODEL=base.en
VEHICLE_API_PORT=8001
WS_PORT=8765
# commuter = brief intro; demo = fuller showcase intro
RIDER_TYPE=commuter
# Default for weather queries (city name or lat,lon)
WEATHER_DEFAULT_LOCATION=San Francisco
# Optional: stream URL when user asks for music. If unset, DEFAULT_MUSIC_STREAM_URL is used (KEXP by default).
# MUSIC_STREAM_URL=
# DEFAULT_MUSIC_STREAM_URL=https://streams.kexp.org/kexp128.mp3

# Spotify: play from full catalog. Requires Premium. Get refresh_token: python scripts/spotify_auth.py
# Create app at https://developer.spotify.com/dashboard, add redirect http://127.0.0.1:8767/callback
# SPOTIFY_CLIENT_ID=
# SPOTIFY_CLIENT_SECRET=
# SPOTIFY_REFRESH_TOKEN=
# Optional: so Spotify (cabin tab) uses same speakers as agent. macOS: brew install switchaudio-osx, then SwitchAudioSource -a to list names. See SPOTIFY_INTEGRATION.md.
# SPOTIFY_OUTPUT_DEVICE_NAME=
